{
    "name": "@nanahoshi/mona-ui",
    "version": "0.0.1",
    "description": "A very light Angular UI framework",
    "main": "dist/mona-ui/fesm2022/mona-ui.mjs",
    "exports": {
        ".": {
            "types": "./dist/mona-ui/index.d.ts",
            "default": "./dist/mona-ui/fesm2022/mona-ui.mjs"
        },
        "./*": {
            "types": "./dist/mona-ui/src/lib/*/index.d.ts",
            "default": "./dist/mona-ui/fesm2022/*.mjs"
        }
    },
    "typings": "dist/mona-ui/index.d.ts",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "build:dev": "ng build mona-ui --configuration development",
        "build:prod": "ng build mona-ui --configuration production",
        "build:watch": "ng build mona-ui --watch --configuration development",
        "watch": "ng build mona-ui --watch --configuration development",
        "test:lib": "ng test mona-ui --code-coverage --watch=false",
        "test:lib:watch": "ng test mona-ui --code-coverage --watch",
        "test:app": "ng test mona-ui-tester"
    },
    "author": "Phrolova",
    "files": [
        "dist",
        "README.md"
    ],
    "dependencies": {
        "@angular/cdk": "^20.0.3",
        "@angular/common": "^20.0.0",
        "@angular/compiler": "^20.0.0",
        "@angular/core": "^20.0.0",
        "@angular/forms": "^20.0.0",
        "@angular/platform-browser": "^20.0.0",
        "@angular/router": "^20.0.0",
        "@mirei/ts-collections": "^15.4.4",
        "@tailwindcss/postcss": "^4.1.10",
        "class-variance-authority": "^0.7.1",
        "luxon": "^3.6.1",
        "postcss": "^8.5.6",
        "rxjs": "~7.8.0",
        "tailwind-merge": "^3.3.1",
        "tailwindcss": "^4.1.10",
        "tslib": "^2.3.0",
        "uuid": "^11.1.0"
    },
    "devDependencies": {
        "@angular-eslint/builder": "^20.1.1",
        "@angular-eslint/eslint-plugin": "^20.1.1",
        "@angular-eslint/eslint-plugin-template": "^20.1.1",
        "@angular-eslint/schematics": "^20.1.1",
        "@angular-eslint/template-parser": "^20.1.1",
        "@angular/build": "^20.0.3",
        "@angular/cli": "^20.0.3",
        "@angular/compiler-cli": "^20.0.0",
        "@eslint/eslintrc": "^3.3.1",
        "@eslint/js": "^9.29.0",
        "@types/jasmine": "~5.1.0",
        "@types/luxon": "^3.6.2",
        "@types/uuid": "^10.0.0",
        "@typescript-eslint/eslint-plugin": "^8.34.1",
        "@typescript-eslint/parser": "^8.34.1",
        "eslint": "^9.29.0",
        "jasmine-core": "~5.7.0",
        "karma": "~6.4.0",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.1.0",
        "ng-packagr": "^20.0.0",
        "prettier": "^3.5.3",
        "typescript": "~5.8.2"
    }
}
